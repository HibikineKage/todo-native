{
  "name": "todo-native",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/react": "^16.4.11",
    "@types/react-native": "^0.56.9",
    "@types/react-redux": "^6.0.6",
    "@types/react-test-renderer": "^16.0.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-preset-airbnb": "^2.5.3",
    "babel-preset-react-native": "^4.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "jest": "^23.5.0",
    "jest-expo": "~27.0.0",
    "prettier": "^1.14.2",
    "react-native-scripts": "1.14.0",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.3.1",
    "ts-jest": "^23.1.3",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start && npm run lint-watch",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android && npm run lint-watch",
    "ios": "react-native-scripts ios && npm run lint-watch",
    "lint-watch": "esw -w --changed app/**",
    "lint": "esw app/**",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^6.3.1",
    "expo": "^27.0.1",
    "native-base": "^2.7.2",
    "react": "16.3.1",
    "react-native": "~0.55.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "typescript-fsa": "^3.0.0-beta-2",
    "typescript-fsa-redux-thunk": "^1.0.1"
  }
}
