{
  "compilerOptions": {
    "target": "es2015",
    "jsx": "react",
    "noEmit": true,
    "strict": true,
    "rootDir": ".",
    "baseUrl": ".",
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "moduleResolution": "node"
  },
  "exclude": [
    "node_modules"
  ]
}